{
  "eq": {
    "prefix": ["eq"],
    "body": ["\\$\\$", "$1", "\\$\\$ {#eq-${2: label}}"],
    "description": "Equation"
  },
  "Insert Block Equation": {
    "prefix": "$$",
    "body": ["\\$$", "${0}", "\\$$"],
    "description": "Equation block"
  },
  "img": {
    "prefix": ["img"],
    "body": ["![](./img/$1)"],
    "description": "Image link"
  },
  "url": {
    "prefix": ["url-link"],
    "body": ["[$1]($2)"],
    "description": "URL link"
  },
  "br": {
    "prefix": ["br"],
    "body": ["<br/>"],
    "description": "Break"
  },
  "py-code": {
    "prefix": ["pyc"],
    "body": ["```python ", "$1", "```"],
    "description": "Python code"
  },
  "bash-code": {
    "prefix": ["bac"],
    "body": ["```bash ", "$1", "```"],
    "description": "Bash code"
  },
  "powershell-code": {
    "prefix": ["pwc"],
    "body": ["```powershell ", "$1", "```"],
    "description": "Powershell code"
  },
  "todo": {
    "prefix": ["todo"],
    "body": ["* [ ] ${1:task} -- due:${2:today} ${3: project:}"],
    "description": "TODO"
  },
  "callout-note": {
    "prefix": ["call-note"],
    "body": ["> [!note]+ $1", "> $0"]
  },
  "callout-info": {
    "prefix": ["call-info"],
    "body": ["> [!info]+ $1", "> $0"]
  },
  "callout-todo": {
    "prefix": ["call-todo"],
    "body": ["> [!todo]+ $1", "> $0"]
  },
  "callout-tip": {
    "prefix": ["call-tip"],
    "body": ["> [!tip]+ $1", "> $0"]
  },
  "callout-abstract": {
    "prefix": ["call-abstract"],
    "body": ["> [!abstract]+ $1", "> $0"]
  },
  "callout-question": {
    "prefix": ["call-question"],
    "body": ["> [!question]+ $1", "> $0"]
  },
  "callout-quote": {
    "prefix": ["call-quote"],
    "body": ["> [!quote]+ $1", "> $0"]
  },
  "callout-example": {
    "prefix": ["call-example"],
    "body": ["> [!example]+ $1", "> $0"]
  },
  "callout-success": {
    "prefix": ["call-success"],
    "body": ["> [!success]+ $1", "> $0"]
  },
  "callout-warning": {
    "prefix": ["call-warning"],
    "body": ["> [!warning]+ $1", "> $0"]
  },
  "callout-failure": {
    "prefix": ["call-failure"],
    "body": ["> [!failure]+ $1", "> $0"]
  },
  "callout-danger": {
