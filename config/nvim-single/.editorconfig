# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

# Matches multiple files with brace expansion notation
[*.{py,c,cpp,java,js,cs}]
charset = utf-8
indent_style = space
indent_size = 4



